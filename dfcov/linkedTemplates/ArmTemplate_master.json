{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dfcov"
		},
		"ls_blob_population_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_blob_population'"
		},
		"ls_cases_and_deaths_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_cases_and_deaths'"
		},
		"ls_dl_population_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_dl_population'"
		},
		"ls_cases_and_deaths_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sqlserverdb0406.database.windows.net"
		},
		"ls_cases_and_deaths_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sqlcovid"
		},
		"ls_cases_and_deaths_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "system"
		},
		"ls_dl_population_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://covdl.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "dfcov_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_blob_population_connectionString": {
						"value": "[parameters('ls_blob_population_connectionString')]"
					},
					"ls_cases_and_deaths_password": {
						"value": "[parameters('ls_cases_and_deaths_password')]"
					},
					"ls_dl_population_accountKey": {
						"value": "[parameters('ls_dl_population_accountKey')]"
					},
					"ls_cases_and_deaths_properties_typeProperties_server": {
						"value": "[parameters('ls_cases_and_deaths_properties_typeProperties_server')]"
					},
					"ls_cases_and_deaths_properties_typeProperties_database": {
						"value": "[parameters('ls_cases_and_deaths_properties_typeProperties_database')]"
					},
					"ls_cases_and_deaths_properties_typeProperties_userName": {
						"value": "[parameters('ls_cases_and_deaths_properties_typeProperties_userName')]"
					},
					"ls_dl_population_properties_typeProperties_url": {
						"value": "[parameters('ls_dl_population_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}